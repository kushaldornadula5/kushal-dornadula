#include <iostream>
using namespace std;

// Function to swap numbers (call by value)
void swap(int x, int y) {
    int temp = x;
    x = y;
    y = temp;
    cout << "Inside function after swap: x = " << x << ", y = " << y << endl;
}

int main() {
    int a, b;
    cout << "Enter first number: ";
    cin >> a;
    cout << "Enter second number: ";
    cin >> b;
    cout << "Before swapping: a = " << a << ", b = " << b << endl;
    swap(a, b);
    cout << "After function call: a = " << a << ", b = " << b << endl;
    return 0;
}
